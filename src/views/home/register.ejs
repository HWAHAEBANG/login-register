<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/home/login.css" />
    <script src="/js/home/register.js" defer></script>
    <title>Document</title>
  </head>
  <body>
    <div class="login-box">
      <h2>Register</h2>
      <form>
        <div class="user-box">
          <input id="id" type="text" name="" required="" />
          <label>ID</label>
        </div>
        <div class="user-box">
          <input id="name" type="text" name="" required="" />
          <label>Name</label>
        </div>
        <div class="user-box">
          <input id="psword" type="password" name="" required="" />
          <label>Password</label>
        </div>
        <div class="user-box">
          <input id="confirm-psword" type="password" name="" required="" />
          <label>Confirm Password</label>
        </div>
        <div>
          <a id="button" href="#">
            <span></span>
            <span></span>
            <span></span>
            <span></span>
            Submit
          </a>
          <div id="register">
            <span>Already registered?</span>
            <a href="/login">Login now</a>
          </div>
        </div>
      </form>
    </div>
  </body>
</html>
